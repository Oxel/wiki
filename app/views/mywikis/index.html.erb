<%= link_to "New wiki", new_mywiki_path %>

<h1>Wiki#index</h1>
<p>Find me in app/views/mywikis/index.html.erb</p>

<%= @mywikis.each do |mywiki| %>
	<h3><%= mywiki.title %></h3>
	<p><%= mywiki.body %><p>
	<%= link_to "Delete", mywiki, method: :delete, data: {confirm: 'Are you certain you want to delete this wiki?'}, remote: true %>
<% end %>






